{
    "manifest_version": 3,
    "name": "Navigation Stats",
    "description": "Chrome extension showing statistics about your navigation.",
    "version": "1.0",
    "action":{
        "default_popup": "popup.html",
        "default_title": "Navigation Stats"
    },
    "permissions": [
        "activeTab",
        "serviceWorker",
        "webNavigation",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16" : "icon.png",
        "32" : "icon.png",
        "48" : "icon.png",
        "128" : "icon.png"
    }
}
